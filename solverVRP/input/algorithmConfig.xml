<?xml version="1.0" ?>

<algorithm xmlns="http://www.w3schools.com"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3schools.com algorithm_schema.xsd">

	<iterations>1024</iterations>
	
	<construction>
		<insertion name="fixedBestInsertion">
			<considerFixedCosts weight="1.0">true</considerFixedCosts>
		</insertion>
	</construction>
	
	<strategy>
		<memory>5</memory>
		<searchStrategies>

			<searchStrategy name="randomRuinAndRecreate">
				<selector name="selectBest"/>
	  			<acceptor name="greedyAcceptance"/> 
				<modules>
					<module name="ruin_and_recreate">
						<ruin name="sequentialRuin">
							<share>0.4</share>
						</ruin>
						<insertion name="fixedBestInsertion"/>
					</module>
				</modules>
				<probability>0.6</probability>
			</searchStrategy>
			
		 	<searchStrategy name="radialRuinAndRecreate">
				<selector name="selectPseudoBest"/>
				<acceptor name="schrimpfAcceptance"> 
				 	<alpha>0.9</alpha>  
  					<warmup>250</warmup> 
  				</acceptor>
				<modules>
					<module name="ruin_and_recreate">
						<ruin name="clusterRuin">
							<share>0.4</share>
						</ruin>
						<insertion name="fixedBestInsertion"/>
					</module>
				</modules>
				<probability>0.4</probability> 
			</searchStrategy>
		</searchStrategies>		
	</strategy>
	

</algorithm>
